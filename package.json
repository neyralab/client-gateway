{
  "name": "gdgateway-client",
  "version": "1.3.16",
  "description": "",
  "type": "module",
  "scripts": {
    "release": "node release.js",
    "build": "npm run es6 && npm run es5",
    "postinstall": "npm run build",
    "es5": "rimraf lib && tsc && tsc --build tsconfig.es5.json",
    "es6": "rimraf lib && tsc && tsc --build tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ghost-Drive/gdgateway-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ghost-Drive/gdgateway-client/issues"
  },
  "homepage": "https://github.com/Ghost-Drive/gdgateway-client#readme",
  "dependencies": {
    "@ipld/car": "^5.3.0",
    "@peculiar/webcrypto": "1.4.4",
    "axios": "^1.4.0",
    "axios-mock-adapter": "^1.21.5",
    "base64-js": "^1.5.1",
    "ipfs-unixfs-exporter": "13.0.6",
    "jest-environment-jsdom": "^29.6.2",
    "node-forge": "^1.3.1",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.11.26",
    "@types/node-forge": "^1.3.4",
    "@types/set-cookie-parser": "^2.4.3",
    "babel-loader": "^9.1.3",
    "cli-table3": "^0.6.3",
    "glob": "^10.3.10",
    "jest": "^29.6.2",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6"
  },
  "react-native": {
    "fs": "react-native-fs",
    "crypto": "react-native-crypto"
  },
  "browser": {
    "stream": "stream-browserify"
  },
  "main": "./lib/es5/index.js",
  "module": "./lib/es6/index.js",
  "types": "./lib/es6/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/es6/index.js",
      "require": "./lib/es5/index.js"
    },
    "./utils/*": {
      "import": "./lib/es6/utils/*.js",
      "require": "./lib/es5/utils/*.js"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
